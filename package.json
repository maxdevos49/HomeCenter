{
    "name": "homecenter",
    "version": "1.0.0",
    "description": "Home/Apartment infotainment center with module extensions planned such as audio, video, picture, and a calender extensions.",
    "main": "server.js",
    "devDependencies": {
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.1",
        "@types/express-session": "^1.15.12",
        "@types/ip": "^1.1.0",
        "@types/mocha": "^5.2.6",
        "@types/mongoose": "^5.3.24",
        "@types/node": "^11.10.4",
        "@types/nodemailer": "^4.6.7",
        "@types/passport": "^1.0.0",
        "@types/passport-local": "^1.0.33",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "chai": "^4.2.0",
        "mocha": "^6.1.3",
        "nodemon": "^1.18.10",
        "socket.io-client": "^2.2.0"
    },
    "dependencies": {
        "@okta/oidc-middleware": "^2.0.0",
        "dotenv": "^6.2.0",
        "esm": "^3.2.9",
        "express": "^4.16.4",
        "express-session": "^1.15.6",
        "ip": "^1.1.5",
        "mongoose": "^5.4.20",
        "nodemailer": "^6.1.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "socket.io": "^2.2.0",
        "source-map-support": "^0.5.11",
        "typescript": "^3.4.3",
        "vash": "^0.12.6"
    },
    "scripts": {
        "setup": "./scripts/dotenv.sh -f && npm i",
        "build": "tsc",
        "start": "node -r esm -r source-map-support/register src/server.js",
        "devstart": "node --require esm --require source-map-support/register --require dotenv/config src/server.js",
        "dev": "nodemon"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maxdevos49/HomeCenter.git"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "exec": "npm run build && npm run devstart",
        "ext": "ts"
    },
    "author": "Maxwell DeVos",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/maxdevos49/HomeCenter/issues"
    },
    "homepage": "https://github.com/maxdevos49/HomeCenter#readme"
}
